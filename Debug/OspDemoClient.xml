<?xml version="1.0" encoding="utf-8"?>
<Window mininfo="450,500" size="500,700">
  <Font name="幼圆" size="16" default="true" />
  <VerticalLayout bkcolor="#FFC0C0C0">
	<HorizontalLayout height="80">
		<VerticalLayout>
			<HorizontalLayout height="25">
				<Edit name="EditIPAddr" text="172.16.160.113" bkcolor="FFF0F8FF" width="160"  height="25"/>
				<VerticalLayout width="80"/>
				<Edit name="EditMsg" text="message box" bkcolor="FFF0F8FF" width="180"  height="25"/>
			</HorizontalLayout>
			<HorizontalLayout height="25">
				<Edit name="EditPort" text="6682" bkcolor="FFF0F8FF" width="80" height="25"/>
				<Button name="ConnectButton" text="连接按钮" normalimage="OspDemoSkins\button_normal.png" hotimage="OspDemoSkins\button_hover.png" pushedimage="OspDemoSkins\button_pushed.png" width="80" height="25" />
				<Button name="DisCntButton"  text="断连按钮" normalimage="OspDemoSkins\button_normal.png" hotimage="OspDemoSkins\button_hover.png" pushedimage="OspDemoSkins\button_pushed.png" width="80" height="25" />
			</HorizontalLayout>
			<HorizontalLayout />
		</VerticalLayout>
	</HorizontalLayout>
	
	<HorizontalLayout height="220">
		<VerticalLayout>
			<HorizontalLayout height="30">
				<Edit name="EditFileSel" text="" bkcolor="FFFFFFFF" width="160" height="25" />
				<Button name="FileSelButton" text="文件选择" normalimage="OspDemoSkins\button_normal.png" hotimage="OspDemoSkins\button_hover.png" pushedimage="OspDemoSkins\button_pushed.png" maxwidth="80" height="25" />
			</HorizontalLayout>
			
			<HorizontalLayout height="30">
				<Button name="FilePstButton" text="发送文件" normalimage="OspDemoSkins\button_normal.png" hotimage="OspDemoSkins\button_hover.png" pushedimage="OspDemoSkins\button_pushed.png" width="80" height="25" />
				<Button name="FileStpButton" text="暂停发送" normalimage="OspDemoSkins\button_normal.png" hotimage="OspDemoSkins\button_hover.png" pushedimage="OspDemoSkins\button_pushed.png" width="80" height="25" />
				<Button name="FileCclButton" text="取消发送" normalimage="OspDemoSkins\button_normal.png" hotimage="OspDemoSkins\button_hover.png" pushedimage="OspDemoSkins\button_pushed.png" width="80" height="25" />
				<HorizontalLayout width="20"/>
				<Option name="OptionDemo1" text="上传列表" width="80" height="25" normalimage="OspDemoSkins\tab_normal.bmp" hotimage="OspDemoSkins\tab_hot.bmp" pushedimage="OspDemoSkins\tab_select.bmp" group="tabDemo" selected="true" />
				<Option name="OptionDemo2" text="完成列表" width="80" height="25" normalimage="OspDemoSkins\tab_normal.bmp" hotimage="OspDemoSkins\tab_hot.bmp" pushedimage="OspDemoSkins\tab_select.bmp" group="tabDemo" />
			</HorizontalLayout>
			<!--Progress name="ProgressDemo1" text="Progress" width="200" height="20" foreimage="OspDemoSkins\progress_fore.png" min="0" max="100" value="0" hor="true" align="center" /-->
			<TabLayout name="tabTest" height="140" bkcolor="#FFF0F8FF">
				<List name="LoadList" vscrollbar="true" hscrollbar="true" height="140" bkcolor="#FFF0F8FF" itemtextcolor="#FF000000" itembkcolor="#FFE2DDDF" itemselectedtextcolor="#FF000000" itemselectedbkcolor="#FFC1E3FF" itemhottextcolor="#FF000000" itemhotbkcolor="#FFE9F5FF" itemdisabledtextcolor="#FFCCCCCC" itemdisabledbkcolor="#FFFFFFFF" >
					<ListHeader name="LoadListHeader" height="20">
						<ListHeaderItem text="文件名称" bkimage="OspDemoSkins\ListHead.png" width="300" height="20" minwidth="250" maxwidth="400" sepwidth="1" align="center" hotimage="List/list_header_hot.png" pushedimage="List/list_header_pushed.png" sepimage="List/list_header_sep.png" />
						<ListHeaderItem text="操作" bkimage="OspDemoSkins\ListHead.png" width="60" height="20" minwidth="60" maxwidth="60" sepwidth="1" align="center" hotimage="List/list_header_hot.png" pushedimage="List/list_header_pushed.png" sepimage="List/list_header_sep.png" />
					</ListHeader>
				</List>
				
				<List name="DoneList" vscrollbar="true" hscrollbar="true" height="140" bkcolor="#FFF0F8FF" itemtextcolor="#FF000000" itembkcolor="#FFE2DDDF" itemselectedtextcolor="#FF000000" itemselectedbkcolor="#FFC1E3FF" itemhottextcolor="#FF000000" itemhotbkcolor="#FFE9F5FF" itemdisabledtextcolor="#FFCCCCCC" itemdisabledbkcolor="#FFFFFFFF" >
					<ListHeader name="DoneListHeader" height="20">
						<ListHeaderItem text="文件名称" bkimage="OspDemoSkins\ListHead.png" width="300" height="20" minwidth="250" maxwidth="400" sepwidth="1" align="center" hotimage="List/list_header_hot.png" pushedimage="List/list_header_pushed.png" sepimage="List/list_header_sep.png" />
						<ListHeaderItem text="状态" bkimage="OspDemoSkins\ListHead.png" width="60" height="20" minwidth="60" maxwidth="60" sepwidth="1" align="center" hotimage="List/list_header_hot.png" pushedimage="List/list_header_pushed.png" sepimage="List/list_header_sep.png" />
					</ListHeader>
				</List>
			</TabLayout>
			<HorizontalLayout />
		</VerticalLayout>
	</HorizontalLayout>
	
	<RichEdit name="EditRecv" bordercolor="#FF000000" bordersize="0" borderround="18,18" inset="4,2,4,2" bkcolor="#FFAAAAA0" bkcolor2="#FFF3F3F0" bkcolor3="#FFAAAAA0" font="1" multiline="true" vscrollbar="true" autovscroll="true" enabled="true" rich="true" readonly="false" text="<内容显示>">
	</RichEdit>
	<Edit name="EditPost" height="50" bordercolor="#FF0000" bordersize="0" borderround="18,18" inset="4,2,4,2" bkcolor="#FFAAAAA0" bkcolor2="#FFF3F3F0" bkcolor3="#FFAAAAA0" font="1" multiline="true" vscrollbar="true" autovscroll="true" enabled="true" rich="true" readonly="false" text="<输入待发送的消息内容...>">
	</Edit>
	
	<HorizontalLayout height="30">
		<Button name="PostButton" height="25" text="发送" normalimage="OspDemoSkins\button_normal.png" hotimage="OspDemoSkins\button_hover.png" pushedimage="OspDemoSkins\button_pushed.png" maxwidth="80" />
		<HorizontalLayout />
		<Button name="CloseButton" height="25" text="退出" normalimage="OspDemoSkins\button_normal.png" hotimage="OspDemoSkins\button_hover.png" pushedimage="OspDemoSkins\button_pushed.png" maxwidth="80" />
	</HorizontalLayout>
  </VerticalLayout>
</Window>